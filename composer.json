{
  "name": "livebuzz/essentials-sdk",
  "description": "SDK Essentials By Jordan Dobrev",
  "keywords": [
    "essentials",
    "laravel",
    "php",
    "sdk"
  ],
  "license": "MIT",
  "type": "package",
  "require": {
    "php": "^7.1",
    "guzzlehttp/guzzle": "~6.0",
    "illuminate/support": "^5.4",
    "nesbot/carbon": "~1.21"
  },
  "require-dev": {
    "phpunit/phpunit": "~6.0",
    "phpspec/phpspec": "~2.1",
    "mockery/mockery": "*",
    "fzaninotto/faker": "1.4.0",
    "squizlabs/php_codesniffer": "3.*",
    "theseer/phpdox": "*"
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php"
    ],
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "autoload": {
    "psr-4": {
      "Buzz\\EssentialsSdk\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "config": {
    "preferred-install": "dist"
  }
}
