{
  "name": "livebuzz/essentials-sdk",
  "description": "SDK Essentials",
  "keywords": [
    "essentials",
    "laravel",
    "php",
    "sdk"
  ],
  "license": "MIT",
  "type": "package",
  "require": {
    "php": "^8.0",
    "guzzlehttp/guzzle": "~7.0",
    "illuminate/support": "^10.0",
    "nesbot/carbon": "^2.16",
    "laravel/helpers": "^1.6"
  },
  "require-dev": {
    "phpunit/phpunit": "~10.0",
    "mockery/mockery": "*",
    "fakerphp/faker": "^1.9.1",
    "squizlabs/php_codesniffer": "3.*"
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php"
    ],
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "autoload": {
    "psr-4": {
      "Buzz\\EssentialsSdk\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "config": {
    "preferred-install": "dist"
  }
}
